{
  "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\$\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)",
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "`", "close": "`" },
    { "open": "\"", "close": "\"" },
    { "open": "<", "close": ">", "notIn": ["string"] },
    { "open": "{>>", "close": "<<}" },
    { "open": "{++", "close": "++}" },
    { "open": "{--", "close": "--}" },
    { "open": "{~~", "close": "~~}" },
    { "open": "{==", "close": "==}" }
  ],
  "surroundingPairs": [
    ["(", ")"],
    ["[", "]"],
    ["`", "`"],
    ["_", "_"],
    ["*", "*"],
    ["{", "}"],
    ["'", "'"],
    ["\"", "\""],
    ["<", ">"],
    ["~", "~"],
    ["$", "$"],
    ["=", "="],
    ["+", "+"],
    ["-", "-"]
  ]
}
